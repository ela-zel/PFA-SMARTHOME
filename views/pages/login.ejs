	<title>SMARTHOME - MARELLA</title>
	<!-- jQuery -->
	<script
	src="https://code.jquery.com/jquery-3.6.0.min.js"
	integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
	crossorigin="anonymous"></script>
	<!-- Bootstrap core CSS with custom theme variables + Additional theme styles -->
    <link href="assets/css/iot-theme-bundle.min.css" rel="stylesheet">
	<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">

</head>

<body class="pt-0">
	<div id="login" class="d-flex align-items-center pb-4">
		
		<div class="row mx-auto" style="width:100%;max-width:24rem;">
			<div class="col-12 mb-2 text-center">
				<img src="assets/images/logo.png" alt="MARELLA" width="141" height="41" class="mx-auto">
			</div>
			<div class="col-12">
				<!-- Login form START -->
				<div class="card px-4 pt-2">
					<% if(typeof msg != 'undefined'){%>
						<%= msg %>		  
					<%}%>
					
					<form action="/login" method="POST" class="p-2">
						<div class="form-group row">
							<label for="user-email" class="col-12 col-form-label">Email</label>
							<div class="col-12">
								<input class="form-control custom-focus" type="text" value="Ela" name="email" id="user-email">
							</div>
						</div>
						<div class="form-group row">
							<label for="user-password" class="col-12 col-form-label">Password</label>
							<div class="col-12">
								<input class="form-control custom-focus" type="password" value="professor" name="password" id="user-password">
							</div>
						</div>
						<div class="form-group row">
							<div class="col-12 mt-3 mb-2 text-center">
		
								<button type="submit" class="btn btn-primary btn-block">Login</button>
								
									
							</div>
						</div>
						
					</form>
				</div>
				<!-- Login form END -->
			</div>
		</div>
	</div>

	<!-- Bootstrap core JavaScript
    ================================================== -->
	<!-- Placed at the end of the document so the pages load faster -->

	<!-- Bootstrap bundle -->
	<script src="assets/js/bootstrap.bundle.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.2/js/toastr.min.js"></script>
<script>
	function login() {
			$("#login").submit(function(e){
    e.preventDefault();
  });
  console.log("login")
		var email=$("#user-email").val()
		var password=$("#user-password").val()
		$.ajax({
      url: "/api/login",
      method: "post",
      data: {
		email:email,
		password:password
      },
      success: function (response) {
		toastr.success('Successfully logged in !');
		window.location = '/';
      },
      error: function (request, error) {
		toastr.danger(error);
      },
    });
	}
</script>
</body>


